<?xml version="1.0" standalone="no"?>
<!DOCTYPE svg>
<svg xmlns="http://www.w3.org/2000/svg">
    <defs>
        <font id="<%= meta['Postscript name'] %>">
            <font-face font-family="<%= meta['Postscript name'] %>"
                       units-per-em="<%= meta.head.unitsPerEm %>"
                       ascent="<%= meta.hhea.ascender %>"
                       descent="<%= meta.hhea.descender %>"
                    />
            <missing-glyph horiz-adv-x="0" />
            <% for (var idx = 0; idx < glyphs.length; idx++) { %>
            <glyph unicode="<%= glyphs[idx].unicode %>" horiz-adv-x="<%= glyphs[idx].boundingBox.upX %>" d="<%= glyphs[idx].path %>"/> <% } %>
        </font>
    </defs>
</svg>
